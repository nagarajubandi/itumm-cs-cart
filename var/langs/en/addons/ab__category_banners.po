msgctxt "Languages::tt_addons_ab__category_banners_views_ab__category_banners_update_ab__cb.form.position"
msgid "tt_addons_ab__category_banners_views_ab__category_banners_update_ab__cb.form.position"
msgstr "Position of the banner, which can be specified in the following ways<br />"
"<ul>"
"<li><code>7</code> - single position</li>"
"<li><code>1,5,7,9</code> - multiple positions</li>"
"<li><code>1-10</code> - range of positions</li>"
"</ul>"
"You can use combinations of these options. For example: <br />"
"<code>1-5,7-9,11,13</code> - the banner will be shown in one of the positions: 1, 2, 3, 4, 5, 7, 8, 9, 11, 13"

msgctxt "Languages::tt_addons_ab__category_banners_views_ab__category_banners_update_ab__cb.form.rand_position"
msgid "tt_addons_ab__category_banners_views_ab__category_banners_update_ab__cb.form.rand_position"
msgstr "If checked, the banner position is selected from the specified positions randomly, otherwise it follows the specified order"

msgctxt "Languages::tt_addons_ab__category_banners_views_ab__category_banners_update_ab__cb.form.monday"
msgid "tt_addons_ab__category_banners_views_ab__category_banners_update_ab__cb.form.monday"
msgstr "Specify the banner active time for this day 'form' and 'till'.<br>"
"If start time not specified, then it is equal 00:00<br>"
"If end time not specified, then it is equal 24:00<br>"
"If both values not specified, then banner displayed from 00:00 till 24:00"

